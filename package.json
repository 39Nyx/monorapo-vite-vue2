{
  "name": "monorapo-vite-vue2",
  "version": "0.0.0",
  "scripts": {
    "dev:examples": "pnpm --filter examples dev",
    "preview:examples": "pnpm run build:lib && pnpm --filter examples preview",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "build:lib": "pnpm --filter @39nyx/* run build"
  },
  "dependencies": {
    "vue": "^2.7.7",
    "vue-router": "^3.5.4"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.2",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "terser": "^5.14.2",
    "vite": "^4.2.3"
  }
}
